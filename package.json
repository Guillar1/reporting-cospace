{
  "name": "itwin-cospace",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "build": "lage build",
    "build:clean": "lage build --no-cache",
    "build:profile": "lage build --no-cache --profile",
    "clean": "lage clean --no-cache",
    "purge": "lage clean --no-cache && cospace purge && pnpm i -w cospace",
    "setOverrides": "cospace override",
    "start:viewer": "pnpm start --dir ./repos/viewer"
  },
  "packageManager": "pnpm@6.32.3",
  "devDependencies": {
    "cospace": "^0.4.5",
    "lage": "1.5.1"
  },
  "pnpm": {
    "overrides": {
      "@itwin/grouping-mapping-widget": "workspace:*",
      "@itwin/reports-config-widget-react": "workspace:*",
      "@itwin/appui-abstract": "3.5.6",
      "@itwin/appui-layout-react": "3.5.6",
      "@itwin/appui-react": "3.5.6",
      "@itwin/build-tools": "3.5.6",
      "@itwin/components-react": "3.5.6",
      "@itwin/core-bentley": "3.5.6",
      "@itwin/core-common": "3.5.6",
      "@itwin/core-frontend": "3.5.6",
      "@itwin/core-geometry": "3.5.6",
      "@itwin/core-i18n": "3.5.6",
      "@itwin/core-markup": "3.5.6",
      "@itwin/core-orbitgt": "3.5.6",
      "@itwin/core-quantity": "3.5.6",
      "@itwin/core-react": "3.5.6",
      "@itwin/eslint-plugin": "3.5.6",
      "@itwin/imodel-components-react": "3.5.6",
      "@itwin/presentation-common": "3.5.6",
      "@itwin/presentation-components": "3.5.6",
      "@itwin/presentation-frontend": "3.5.6",
      "@itwin/webgl-compatibility": "3.5.6"
    },
    "prefer-workspace-packages": true,
    "link-workspace-packages": "deep"
  }
}
